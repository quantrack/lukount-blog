<section id="recientes">
    <div class="container-fluid">
<h2>Recientes</h2>
<div class="row">

    <%  posts.slice().reverse().forEach(function(post){ %>
<div class="blog-column col-lg-4 col-md-6">
    <div class="card-content">
        <div class="pbimg">
        <a href="/post/<%=post.title%>"><img class = "blog-img card-img-top mx-auto"src="<%=post.image%>" alt="blog_img"></a>
    </div>
        <div class="card-desc">
            
            <a href="/post/<%=post.title%>"><h3><%= post.title %> </h3></a>

            <a href="/post/<%=post.title%>"><p class="cardp"><%=post.subtitle%> </p></a>
            <!-- .substring(0, 100) + " ..."  -->
            
        </div>
    </div>
</div>
<% }) %>



</div>
<!-- <h3 class="buscar">Buscar más  <i class="fa fa-chevron-down"></i></span></h3>  -->
</div>
</section>




